        "gpgSign": true
    },

    "files": [
        {
            "includePattern": "target/(.*).jar",
            "uploadPattern": "/io/vlingo/vlingo-telemetry/0.8.2/$1.jar",
            "matrixParams": {
                "override": 1
            }
        },
        {
            "includePattern": "pom.xml",
            "uploadPattern": "/io/vlingo/vlingo-telemetry/0.8.2/vlingo-telemetry-0.8.2.pom",
            "matrixParams": {
                "override": 1
            }
        }
    ],

    "publish": true
}

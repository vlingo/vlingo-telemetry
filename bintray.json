        "website_url": "https://github.com/vlingo/vlingo-telemetry",
        "issue_tracker_url": "https://github.com/vlingo/vlingo-telemetry/issues",
        "vcs_url": "https://github.com/vlingo/vlingo-telemetry.git",
        "licenses": ["MPL-2.0"],
        "public_download_numbers": true,
        "public_stats": true
    },

    "version": {
